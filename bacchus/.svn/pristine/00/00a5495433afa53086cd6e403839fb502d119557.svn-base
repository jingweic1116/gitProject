/*Reservation Page*/
.ui-widget{
	font-family:'微軟正黑體';
}


.ui-tabs .ui-tabs-panel{
	padding-top:1%;
	padding-left:1%;
	padding-right:0%;
}

.form-widget{
	border-style:solid; 
	border-color:#C5D0DC; 
	border-width: 1px;
	padding:0;
}


.form-widget-header{
	background-color:#BED2E9;
	border-bottom-style: solid;
	border-bottom-color:#C5D0DC;
	border-bottom-width: 1px; 
	width:100%;
	flood-color:#898F96;
	padding:10px;

}

.form-widget-content{
	padding-bottom:1%;
}

.form-widet-content-notab{
	padding-top:16px;
	padding-left:5px;

}

.form-widget-label{
	width:100%;
}

.nav-tabs.background-blue {
  background-color: #BED2E9 ;
}

.tabs-below .nav-tabs.background-blue {
  padding-top: 0;
  padding-bottom: 0px;
}
.tabs-below .nav-tabs.tab-color-blue {
  border-top: none;
  border-bottom-color: #C5D0DC;
}



.form-control{
	padding:0px;
	height:auto;
	display: inline-block;
}

.group45{
	display:inline-table;
	width:45%;
}

.group45-addon{
    font-size:0.4em;
    font-weight: normal;
    color: #fff;
    text-align: center;
    background-color: #EF7B1B;
    border: 1px solid #ccc;
    border-left-style:none ;
    width:19.5%;
    display:table-cell;
    white-space: nowrap;
    vertical-align: middle;
    margin:0 auto;
}

.group15{
	display:inline-table;
	width:15%;
}
.group-addon{
    font-size: 1em;
    font-weight: normal;
    color: #555555;
    text-align: center;
    background-color: #eeeeee;
    border: 1px solid #ccc;
    border-left-style:none ;
    width: 1%;
    display:table-cell;
    white-space: nowrap;
    vertical-align: middle;
    height:20px;
}
