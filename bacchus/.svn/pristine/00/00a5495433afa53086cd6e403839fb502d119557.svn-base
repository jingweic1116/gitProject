	text-decoration: none;
}

.right-menu-co {
	font-size: 0.8em;
}
.right-menu-co-icon{
	width: 40px;
    height:40px;
    border-radius:40px;
    border:1px solid black;
    margin: 0px auto 4px auto;
    box-shadow: 2px 2px 3px rgba(51, 51, 102, 0.5);
    font-size: 1.8em;
    padding-top:3px;
}

.right-menu-co-icon:hover{
    background-color:#2B7DBC;
    border:1px solid #2B7DBC;
    color:#fff;
}

