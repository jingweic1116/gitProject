input{
  color: #858585;
  background-color: #FFF;
  border: 1px solid #D5D5D5;
  font-size: 1em;
  font-family: inherit;
  -webkit-box-shadow: none !important;
  box-shadow: none !important;
  -webkit-transition-duration: 0.1s;
  transition-duration: 0.1s;
  height:25px;
}
input[type="radio"]{
	padding-top:1px;
}
select {
  -webkit-box-shadow: none !important;
  box-shadow: none !important;
  color: #858585;
  background-color: #FFF;
  border: 1px solid #D5D5D5;
  /*height:25px !important;*/
}


