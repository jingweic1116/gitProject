//int_Counter．html

.counterPage
  //position: relative
  max-height: 600px
  overflow: auto
  .counterRow
    //margin: 0 auto
    .counterBlock
      float: left
      margin: 0 5px 5px 0
      //border: 1px solid #555
      position: relative
      padding: 5px
      //min-width: 100%
      //min-height: 100%
      width: 125px
      height: 70px
      line-height: 23px
      box-sizing: border-box
      background: #c0d6ee
      color: #000
      font-size: 11px
      cursor: pointer
      @include color-bk
      &:hover
        opacity: 0.6
      .head
        .left
          position: absolute
          top: 5px
          left: 5px
        .right
          position: absolute
          top: 5px
          right: 5px
        .txt-lg
          font-weight: bold
          font-size: 18px
          line-height: 18px
      .content
        position: absolute
        top: 25px
        left: 5px
      .content-sub
        .left
          position: absolute
          top: 45px
          left: 5px
      .foot
        position: absolute
        bottom: 5px
        left: 5px
        .red-tip
          span
            color: #ff0000
            //font-weight: bold
            //font-size: 14px
      .foot-right
        position: absolute
        bottom: 5px
        right: 5px
        span
          color: #D15B47

.counter-right
  min-height: 200px
  max-height: 100%
  overflow-y: auto
  label
    font-size: 9px

// int_counter floorPlan-table
.floorPlan-table
  table
    border: 1px solid #C5D0DC
    width: 100%
    th,td
      border: 1px solid #C5D0DC
      padding: 2px 5px
      text-align: center
    tr
      th
        background: #c0d6ee
      td
        word-break: break-all
        .moreInfo
          background: #bfbfbf
          border: none
          float: right
        .float-left
          float: left
        .float-right
          float: right
        &.roomNum
          box-sizing: border-box
          font-size: 1.5em
          font-style: italic
          font-weight: bold
          color: #DD5A43
          border: 2px solid #DD5A43
  .customInfo-table
    width: 100%
    .table-left,.table-right
      float: left
    .table-left
      width: 25%
      margin: 0 3% 0 0
    .table-right
      width: 72%
  .fav
    table
      width: 100%
      th
        width: 20%
      td
        width: 80%

.show-intCounterRight
  display: none
