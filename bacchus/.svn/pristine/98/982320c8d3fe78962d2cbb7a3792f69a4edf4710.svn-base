/*-- 圖片 hover 文字 --*/
.system-block-all
  margin: 50px 0 0 0
.system-block
  margin: 0 0 0 30px
  display: inline-block
.imgWrap
  position: relative
  display: inline-block
  margin: 0 15px 0 0
  span.text-content
    background: rgba(0,0,0,0.5)
    color: white
    cursor: pointer
    display: table
    height: 150px
    position: absolute
    left: 0
    top: 0
    width: 150px
    border-radius: 150px
    opacity: 0
  span.text-content span
    display: table-cell
    text-align: center
    vertical-align: middle
  &:hover .text-content
    visibility: visible
    opacity: 1
