//mixin
@mixin color-bk
  &.cb-yellow
    background: #fffba1
  &.cb-purple
    background: #ae8dc3
  &.cb-green
    background: #7cbd64
  &.cb-pink
    background: #ffddb3
  &.cb-gray
    background: #bfbfbf
  &.cb-brown
    background: #dcbe88
  &.cb-blueGreen
    background: #06eed5
  &.cb-grayGreen
    background: #aac077
  &.cb-default
    background: #c0d6ee
// /.mixin END
