.right-menu-co
  //border-left: 1px solid #ede3d1
  text-align: center
  ul
    margin-left: 0
    margin-bottom: 0
    li

      list-style: none
      padding: 10px 0
      vertical-align: middle
      //border-bottom: 1px solid #ededed
      //&:last-child
      //  border-bottom: none
      div
        width: 48px
        height: 48px
        border-radius: 48px
        margin: 0 auto 8px auto
        box-shadow: 2px 2px 3px rgba(20%,20%,40%,0.5)
        //box-shadow: 2px 2px 1px rgba(20%,20%,40%,0.5) inset
        //box-shadow: 2px 2px 2px rgba(20%,20%,40%,0.6),4px 4px 6px rgba(20%,20%,40%,0.4),6px 6px 12px rgba(20%,20%,40%,0.4)
      span
        font-weight: bold

@mixin icon-bk($imgpath,$imgpathHover,$repeat: no-repeat,$bk-size: 100% 100%)
  background: url($imgpath)
  //position: $position
  repeat: $repeat
  background-size: $bk-size
  &:hover
    background: url($imgpathHover)
    repeat: $repeat
    background-size: $bk-size

.icon-co-03
  @include icon-bk('../images/icon/03.png','../images/icon/03-1.png')
.icon-co-06
  @include icon-bk('../images/icon/06.png','../images/icon/06-1.png')
.icon-co-10
  @include icon-bk('../images/icon/10.png','../images/icon/10-1.png')
.icon-co-11
  @include icon-bk('../images/icon/11.png','../images/icon/11-1.png')
.icon-co-14
  @include icon-bk('../images/icon/14.png','../images/icon/14-1.png')


